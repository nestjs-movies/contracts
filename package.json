{
  "name": "@nestjs-movies/contracts",
  "version": "1.0.8",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto.cmd -I ./proto ./proto/*.proto --ts_proto_out=./gen --ts_proto_opt=nestJs=true"
  },
  "description": "Protobuf definitions and generated Typescript types",
  "dependencies": {
    "@nestjs-movies/common": "^1.0.1",
    "@nestjs/microservices": "^11.1.12",
    "rxjs": "^7.8.2",
    "ts-proto": "^2.11.0"
  },
  "files": [
    "dist",
    "proto",
    "gen"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "typescript": "^5.9.3"
  }
}
